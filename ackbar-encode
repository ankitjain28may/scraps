#!/usr/bin/perl

my @lines = <DATA>;

for my $line ( @lines ) {
    chomp $line;
    print encode_line( $line ), "\n";
}

sub encode_line {
    my $s = shift;

    my $out = '';

    while ( $s =~ /(.)\1*/g ) {
        $out .= $1 . chr(32+length($&));
    }

    return $out;
}

